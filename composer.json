{
    "name": "californiamountainsnake/longmantelegrambot-payments",
    "type": "library",
    "description": "Classes intended for the processing payments via Telegram Payments API using a longman/telegram-bot library.",
    "license": "MIT",
    "authors": [
        {
            "name": "CaliforniaMountainSnake",
            "email": "CaliforniaMountainSnake1@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "californiamountainsnake/php-database-entities": "~1.0.5",
        "longman/telegram-bot": "~0.60",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "CaliforniaMountainSnake\\LongmanTelegrambotPayments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "\"vendor/bin/phpunit\""
        ]
    }
}
